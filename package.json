    "moment": "^2.24.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "commitizen": "^4.0.3",
    "cross-env": "^5.2.1",
    "egg-bin": "^4.13.0",
    "egg-ci": "^1.13.0",
    "egg-mock": "^3.23.1",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-egg": "^7.5.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
