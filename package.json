    "decimal.js": "^10.2.0",
    "egg": "^2.23.0",
    "egg-mysql": "^3.0.0",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.11.0",
    "egg-socket.io": "^4.1.6",
    "egg-validate": "^2.0.2",
    "elliptic": "^6.5.3",
    "dotenv": "^8.2.0",
    "lodash.camelcase": "^4.3.0",
